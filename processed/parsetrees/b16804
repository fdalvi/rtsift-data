          (VP (TO to)
            (VP (VB make)
              (NP (NNS appointments))
              (PP (IN for)
                (NP
                  (NP (NNS massages))
                  (PP (IN at)
                    (NP (DT the) (JJ same) (NN time))))))))))
    (. .)))

(ROOT
  (S
    (NP (DT The) (JJ hot) (NN stone) (NN massage))
    (VP (VBD was)
      (ADJP (JJ amazing)))
    (. !)))

(ROOT
  (S
    (S
      (NP (DT This) (NN place))
      (VP (VBZ does) (RB not)
        (VP (VB disappoint))))
    (CC and)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADJP (RB very) (NN professional))))
    (. .)))


