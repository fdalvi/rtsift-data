                        (NP (DT the) (NN summer) (NNS months)))))
                  (SBAR
                    (WHNP (WP who))
                    (S
                      (VP (MD may)
                        (VP (VB need)
                          (NP
                            (NP (JJ extra) (NN care) (NN becuase))
                            (PP (IN of)
                              (NP (PRP$ their) (NN situation)))))))))))))))
    (. .)))

(ROOT
  (NP (NNP HIGHLY) (NNP RECOMMEND)))


