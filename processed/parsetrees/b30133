      (VP (VBD tried)
        (ADVP (RB again))))
    (. .)))

(ROOT
  (S
    (NP
      (NP (DT The) (NN website))
      (CC and)
      (NP (DT the) (NN phone) (NN number)))
    (VP (VBP are) (RB not)
      (VP (VBG working)))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP WANT)
      (NP (DT A) (JJ REFUND) (NN !!!!!!!!!!!!)))))


