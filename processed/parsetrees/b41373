        (PP (IN with)
          (NP (NN service)))
        (PP (IN like)
          (NP
            (NP (DT this))
            (, ,)
            (NP (PRP$ its)
              (ADJP (JJ hard)
                (S
                  (VP (TO to)
                    (VP (VB give)
                      (NP (DT a) (JJ good))))))
              (NN rating))))))
    (. .)))


