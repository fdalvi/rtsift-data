        (VP (VBG involving)
          (NP (JJ several) (JJR more) (NNS attempts))
          (S
            (VP (TO to)
              (VP (VB call)
                (NP (PRP them)))))))
      (-RRB- -RRB-))
    (, ,)
    (NP (PRP I))
    (VP (VBD ordered)
      (NP (DT a) (NN pizza))
      (PP (IN from)
        (NP (RB somewhere) (RB else))))
    (. .)))

(ROOT
  (S
    (NP (DT The) (JJ whole) (NN situation))
    (VP (VBZ is)
      (ADJP (JJ absurd)))
    (. .)))

