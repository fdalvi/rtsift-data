          (PP (IN after)
            (NP (CD 20) (JJR more) (NNS minutes))))
        (PP (IN after)
          (NP (DT that)))))
    (. .)))

(ROOT
  (NP
    (NP (NNP Bad) (NN service))
    (PP (IN at)
      (NP (DT that) (NNP Gordon) (NNP Biersch)))
    (. .)))

(ROOT
  (FRAG
    (ADJP (RB Truly) (RB truly) (JJ disappointing))
    (. .)))

(ROOT
  (S
    (NP (PRP We))
