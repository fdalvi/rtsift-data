            (NP (PRP you))
            (VP (VBP 're)
              (VP (VBG going)
                (S
                  (VP (TO to)
                    (VP (VB be)
                      (PP (IN in)
                        (NP (DT the) (NN pit) (NN area))))))))))))
    (. .)))

(ROOT
  (S
    (VP
      (VP (VB Drink)
        (NP (PRP$ your) (NNP f) (NNPS *) (VBG cking) (NN drink))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP you))
            (VP (VBP buy)
              (NP (PRP it))))))
      (, ,) (RB then)
      (VP (VB head)
        (PRT (RP back))
        (PP (TO to)
          (NP (DT the) (NN floor)))))
    (. .)))


(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP like)
        (NP
          (NP (DT the) (NN size))
          (PP (IN of)
            (NP (DT this) (NN place))))))
    (: ...)
    (S
      (NP (PRP You))
      (VP (MD ca) (RB n't)
        (ADVP (RB really))
        (VP (VB have)
          (NP (DT a) (JJ bad) (NN seat) (NN :-RRB-)))))))


