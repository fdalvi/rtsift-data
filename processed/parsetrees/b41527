          (QP ($ $) (CD 10) (CD +)))
        (PRN (: -)
          (PP (IN per)
            (NP (NN dish)))
          (PP (IN for)
            (NP (NN dinner)
              (CC and)
              (NN lunch))))))
    (VP (VBZ is)
      (NP (DT the) (JJ obligatory) (NN buffet)))
    (. .)))


