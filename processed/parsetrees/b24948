    (ADVP (RB so))
    (VP (MD could) (RB n't)
      (VP (VB get)
        (NP
          (NP (DT a) (NN hold))
          (PP (IN of)
            (NP (NN shop))))
        (SBAR (IN because)
          (S
            (NP (NN worker))
            (VP (MD could) (RB n't)
              (VP (VB answer)
                (NP (DT the) (NNS phones))))))))
    (. .)))

(ROOT
  (S
    (VP (VB Do) (RB not)
      (VP (VB recommend)))))


