                        (VP (VBP are) (RB not)
                          (PP (IN on)
                            (NP
                              (NP (DT this) (NN side))
                              (PP (IN of)
                                (NP (NN town))))))))))))))))
    (. .)))

(ROOT
  (NP (NNP Jeff)))


