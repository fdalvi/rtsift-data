                            (PP (IN after)
                              (S
                                (VP (VBG having)
                                  (VP (VBN gone)
                                    (ADVP (RB there))
                                    (PP (IN for)
                                      (ADVP (RB so) (RB long)))))))))))))))))))
    (. .)))


