                (NP (JJ good) (NN service))
                (, ,)
                (NP (JJ decent) (NN beer) (NN tap) (NN selection)))))
          (CC and)
          (ADJP (RB very) (JJ yummy)))
        (NN food)))
    (. .)))

(ROOT
  (S
    (NP (PRP We))
    (VP (MD will)
      (ADVP (RB definitely))
      (VP (VB return)
        (S
          (VP (TO to)
            (VP (VB try)
              (NP (JJ other) (NNS items)))))))
    (. .)))


