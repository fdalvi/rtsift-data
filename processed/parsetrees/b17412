                (VP (VBD was)
                  (ADJP (JJ cold)))))))))
    (CC and)
    (S
      (NP (PRP$ my) (NN brother))
      (VP (VBD got)
        (ADJP (JJ sick))
        (ADVP (RB afterward))))
    (. .)))

(ROOT
  (S
    (S
      (NP (PRP I))
