                        (NP (PRP you))
                        (VP (VBP call)
                          (S
                            (VP (TO to)
                              (VP (VB speak)
                                (PP (TO to)
                                  (NP (DT a) (NN manager)))))))))
                    (ADVP (RB apparently))
                    (NP (PRP they))
                    (VP (VBP 're)))))))))
      (VP (VBP are) (RB n't)
        (NP (DT any) (JJ official) (NNS managers)
          (S
            (VP (TO to)
              (VP (VB make)
                (ADJP (JJ aware))
                (PP (IN of)
                  (NP (NNS things)))))))))
    (: ...)
    (FRAG
      (X (WP what))
      (NP (DT a) (NN joke)))
    (. .)))


