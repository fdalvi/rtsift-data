(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (MD 'd)
        (VP (VB go)
          (ADVP (RB again)))))
    (CC but)
    (S
      (NP (PRP I))
      (VP (MD 'll)
        (VP (VB try)
          (NP (NN something) (JJ different))
          (PP (IN than)
            (NP
              (NP (NNP BBQ))
              (: ...) (RB maybe)
              (NP
                (NP (DT a) (NN breakfast) (NN dish))
                (SBAR
                  (WHNP (WDT which))
                  (S
                    (NP (PRP I))
                    (VP (VBP hear)
                      (SBAR
                        (S
                          (NP (PRP they))
                          (VP (VBP are)
                            (ADJP (JJ good)
                              (PP (IN at)))))))))))))))
    (. .)))


