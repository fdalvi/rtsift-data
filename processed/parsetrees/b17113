    (. .)))

(ROOT
  (S
    (NP (NNP Reasonably))
    (VP (VBN priced)
      (PP (IN for)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (PRP you))
            (VP (VBP are)
              (VP (VBG getting)))))))
    (. .)))


