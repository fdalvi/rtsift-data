      (NP (PRP I)))
    (VP (MD would)
      (ADVP (RB definitely))
      (VP (VB hire)
        (NP (PRP them))
        (PP (IN for)
          (NP (DT any) (JJ future) (NN roofing) (NN work)))))
    (. .)))


