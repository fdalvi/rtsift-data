                (ADJP (RB super) (JJ comfortable))))))))
    (. !)))

(ROOT
  (NP
    (S
      (S
        (NP (PRP I))
        (ADVP (RB really))
        (VP (VBD liked)
          (NP (DT this) (NN place))))
      (CC and)
      (S
        (NP (PRP I))
        (VP (VBP recommend)
          (S
            (NP (PRP it)
              (CC or)
              (PRP you))
            (ADJP (CD 100) (NN %))))))))


