    (VP (VBD walked)
      (PRT (RP out))
      (ADJP (JJ great))
      (PP (IN for)
        (NP
          (NP (NN finding) (NNS books))
          (PP (CC but)
            (NP (NNP NEVER)))))
      (VP (VB take)
        (NP (NN anything))
        (PRT (RP in))
        (S
          (VP (TO to)
            (VP (VB sell))))))))


