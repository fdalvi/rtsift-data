                  (ADVP (RB ever))
                  (VP (VBN had))))))
          (: ...)
          (NP
            (NP (DT All))
            (PP (IN in)
              (NP (DT all)))))))
    (, ,)
    (NP (DT this) (NN place))
    (VP (VBZ is)
      (NP (DT a) (JJ solid) (CD 5) (NNS stars)))
    (. .)))

