          (CC and)
          (ADJP (JJ quick)))
        (S
          (VP (TO to)
            (VP (VB resolve)
              (NP (DT any) (NN company) (NN policy) (NN issue))
              (SBAR (IN that)
                (S
                  (NP (PRP I))
                  (VP (MD may)
                    (VP (VB have))))))))))
    (. .)))


