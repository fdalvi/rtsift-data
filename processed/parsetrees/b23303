          (VP (VBZ is)
            (SBAR
              (WHADVP (WRB why))
              (S
                (NP (PRP I))
                (VP (VBP 'm) (RB not)
                  (VP (VBG giving)
                    (NP (PRP it))
                    (NP (CD 1) (NN star))))))))))
    (. .)))

(ROOT
  (S
    (VP (VB Apart)
      (PP (IN from)
        (NP
          (NP (DT that))
          (, ,)
          (ADJP (RB not) (JJ great))))
      (ADVP (RB really)))
    (. .)))


