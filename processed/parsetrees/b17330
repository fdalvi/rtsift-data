(ROOT
  (FRAG (RB Not)
    (VP
      (ADVP (RB much) (RBR more))
      (TO to)
      (VP (VB ask)
        (PP (IN for))))
    (. .)))

(ROOT
  (S
    (NP (NNP Great))
    (VP (VBP place))
    (. .)))


