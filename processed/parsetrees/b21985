      (VP (VBP make)
        (S
          (NP (PRP it))
          (ADJP (RB even) (RBR more) (JJ fun))
          (S
            (VP (TO to)
              (VP (VB dine)
                (ADVP (RB here))))))))
    (. .)))

(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (CD one))
        (PP (IN of)
          (NP (PRP$ my) (JJ favorite) (NNS spots)))))
    (. .)))


