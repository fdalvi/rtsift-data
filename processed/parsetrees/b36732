    (. .)))

(ROOT
  (S
    (ADVP (DT All)
      (PP (IN in)
        (NP (DT all))))
    (, ,)
    (NP (PRP it))
    (VP (VBD was)
      (NP (DT a) (JJ not-so-stellar) (NN experience)))
    (. .)))

(ROOT
  (S
    (NP
      (NP (DT The) (JJ only) (NN thing))
      (VP (VBG saving)
        (NP (DT this) (NN spot))))
    (VP (VBZ is)
      (NP
        (NP (NN location))
        (CC and)
        (NP (DT the) (NNS meatballs))))
    (. .)))


