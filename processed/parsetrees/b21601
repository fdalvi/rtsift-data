                      (NP
                        (NP (DT this) (NN workout))
                        (PP (IN for)
                          (ADJP (JJ sure)))))))))))))
    (. !)))

(ROOT
  (S
    (VP
      (VP (VB Lve)
        (NP (DT the) (NN shake)))
      (CC and)
      (VP (VB burn)))
    (. !)))


