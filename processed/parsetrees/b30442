                                  (VP (VBD included)
                                    (SBAR (IN that) (, ,)
                                      (S
                                        (NP
                                          (NP (DT all))
                                          (PP (IN of)
                                            (NP (PRP$ their) (NN pick) (NN n) (NN mix) (NN stuff))))
                                        (VP (VBD seemed)
                                          (S
                                            (VP (TO to)
                                              (VP (VB be)
                                                (PP (IN in)
                                                  (NP (JJ individual) (NNS wrappers)))))))))))))))))))))))))
    (. .)))


