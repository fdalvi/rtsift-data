              (SBAR
                (S
                  (NP (CC &))
                  (VP (VBD was)
                    (ADJP (JJ honest)
                      (CC and)
                      (JJ knowledgable)))))
              (. .))))))
    (. .)))

(ROOT
  (S
    (S
      (VP (VB Awesome)
        (NP (NN customer) (NN service))))
    (, ,)
    (NP (PRP I))
    (VP (MD 'll)
      (ADVP (RB definitely))
      (VP (VB be)
        (ADVP (RB back))))
