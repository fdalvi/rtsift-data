    (. .)))

(ROOT
  (S (CC But)
    (NP (DT the) (NN lake))
    (VP (VBD mead)
      (SBAR
        (S
          (NP (NN location))
          (VP (VBZ is)
            (ADJP (JJR better))))))
    (. .)))


