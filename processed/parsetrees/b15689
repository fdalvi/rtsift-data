          (NP (DT the) (NNP Henderson) (NN part))
          (PP (IN of)
            (NP
              (NP (DT the) (NN valley))
              (CC and)
              (NP (JJR more) (NNS offerings))))))
      (SBAR
        (WHADVP (WRB when))
        (S
          (VP (VBN compared)
            (PP (TO to)
              (NP (DT the) (NNP Lake) (NNP Las) (NNP Vegas) (NN section)))))))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do)
      (NP (NN love) (NNP Town) (NNP Square))
      (ADVP (RB though))
