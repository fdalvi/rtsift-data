            (. .)))))
    (: -)
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (VP (VBG watching)
          (S
            (NP (PRP you))
            (VP (VB make)
              (NP (PRP$ my) (NN sandwich) (: ...) (NN Ugh)))))))
    (. !)))

