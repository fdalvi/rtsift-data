                (VP (VBG researching)
                  (NP (PRP$ our) (NN claim))))))
          (CC and)
          (VP (VBD kept)
            (SBAR
              (S
                (NP (PRP us))
                (VP (VBZ updated)
                  (NP
                    (NP (DT each) (NN step))
                    (PP (IN of)
                      (NP (DT the) (NN way)))))))))))
    (. .)))


