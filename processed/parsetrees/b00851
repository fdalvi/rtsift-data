    (VP (VBP are)
      (ADJP (RB insanely) (JJ low))
      (, ,)
      (SBAR
        (WHNP (WDT which))
        (S
          (VP (VBZ is)
            (ADJP (JJ great))
            (SBAR (IN if)
              (S
                (NP (PRP you))
                (VP (VBP live)
                  (PP (IN on)
                    (NP (DT a) (JJ tight) (NN budget)))
                  (ADVP (RB sometimes)))))))))
    (. .)))


