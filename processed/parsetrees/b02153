          (SBAR (IN that)
            (S
              (NP (DT this) (NN dealership))
              (VP (VBZ has)
                (VP (VBN exceeded)
                  (NP (PRP$ my) (NNS expectations)))))))))
    (. .)))

(ROOT
  (S
    (VP (VB Thank)
      (NP (PRP you)))
    (. !)))


