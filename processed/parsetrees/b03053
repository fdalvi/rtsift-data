              (ADVP (PDT all) (DT the) (JJ great))
              (ADVP (RB either)))))))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (MD would) (RB not)
      (VP (VB go)
        (ADVP (RB again))))))


