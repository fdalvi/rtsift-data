(ROOT
  (S
    (NP (PRP I))
    (VP (MD wo) (RB n't)
      (VP (VB be)
        (ADVP (RB back))))
    (. .)))

(ROOT
  (S
    (NP (NNP YC) (POS 's))
    (VP (VBZ is)
      (ADJP (RB 100x) (JJR better)))
    (. .)))


