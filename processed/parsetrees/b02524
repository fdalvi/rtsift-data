          (NN price))))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB recommend)
        (PP (TO to)
          (NP
            (NP (NNS friends))
            (CC and)
            (NP (PRP$ my) (NN family))))))
    (. .)))


