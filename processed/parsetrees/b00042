              (PP (IN of)
                (NP (PRP us))))))))
    (. .)))

(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ is)
        (ADVP (RB always))
        (ADJP (JJ wonderful)
          (S
            (VP (TO to)
              (VP (VB get)
                (NP (JJ great) (NN customer) (NN service))))))))
    (CC and)
