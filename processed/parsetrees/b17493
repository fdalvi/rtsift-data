        (NP (JJ much) (NN sense))
        (PP (TO to)
          (NP (PRP me)))
        (SBAR (CC but)
          (S
            (NP (PRP I))
            (VP (MD 'll)
              (ADVP (RB gladly))
              (VP (VB take)
                (NP (PRP$ my) (NN business))
                (ADVP (RB elsewhere))))))))
    (. .)))


