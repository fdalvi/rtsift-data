        (ADJP (JJ prompt))
        (CC and)
        (ADJP (RB very) (JJ nice))))
    (. .)))

(ROOT
  (S
    (NP (JJ Great) (JJ reasonable) (NNS rates))
    (VP
      (ADVP (RB also)))
    (. .)))

(ROOT
  (S
    (NP (NNP Highly))
    (VP (VBP recommend))
    (. !)))


