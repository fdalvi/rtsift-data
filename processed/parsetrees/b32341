                (NP (JJ 7th) (NNP Ave.))))))))))

(ROOT
  (S
    (ADVP (RB Even) (RB so))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (PP (IN worth)
        (NP (DT a) (NN look) (NNP N) (NNP *) (NN joy))))
    (: ...)))


