    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB love)
        (S
          (VP (TO to)
            (VP (VB try)
              (NP (DT the) (JJ hot) (NN food))
              (SBAR (IN if)
