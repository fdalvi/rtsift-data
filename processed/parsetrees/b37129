        (S
          (NP (PRP I))
          (VP (MD will)
            (ADVP (RB definitely))
            (VP (VB stop)
              (PRT (RP by))
              (S
                (VP (TO to)
                  (VP (VB take)
                    (NP (DT some) (NNP Snoh) (NN to-go))
                    (SBAR
                      (WHADVP (WRB when))
                      (S
                        (NP (PRP I))
                        (VP (VBP am)
                          (PP (IN in)
                            (NP (DT the) (NN neighborhood))))))))))))))
    (. !)))


