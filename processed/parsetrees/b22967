    (. .)))

(ROOT
  (S
    (S
      (NP (DT The) (NN place))
      (VP (VBZ is)
        (VP (VBN enjoyed)
          (PP (IN by)
            (NP (DT the) (NNS locals))))))
    (, ,)
    (CC yet)
    (S
      (ADVP (NN nothing))
      (NP (PRP I))
      (VP (MD would)
        (VP (VB recommend)
          (SBAR (IN if)
