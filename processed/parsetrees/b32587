                  (NP (NN something) (JJ other))
                  (PRN (-LRB- -LRB-)
                    (CC and)
                    (ADJP (JJR better))
                    (-RRB- -RRB-))))
              (SBAR (IN than)
                (S
                  (NP (DT the) (NNPS Subways)
                    (CC and)
                    (NNPS Chinese))
                  (VP (VBZ places)
                    (PP (IN in)
                      (NP (DT the) (NN area)))))))))))
    (. .)))


