      (PP (IN on)
        (S
          (VP
            (VP (VBG returning)
              (PP (IN in)
                (NP (PRP$ my) (JJ own))))
            (CC and)
            (VP (VBG updating)
              (NP (PRP$ my) (NN review))
              (PP (IN with)
                (NP (DT that) (NN experience))))))))
    (. .)))


