          (VP (VBZ has)
            (VP (VBN been)
              (VP (VBG attending)
                (NP
                  (NP (DT a) (NN summer) (NN camp))
                  (PP (IN for)
                    (NP (CD three) (NNS years))))))))))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB highly))
    (VP (VBP recommend)
      (NP (DT this) (NN church)))
    (. .)))


