(ROOT
  (S
    (NP (PRP$ My) (NN dad))
    (VP (VBD enjoyed)
      (NP (PRP$ his) (NN lunch))
      (SBAR (IN so)
        (S
          (NP (PRP we))
          (VP (MD will)
            (VP (VB be)
              (ADVP (RB back)))))))
    (. .)))


