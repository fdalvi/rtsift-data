      (ADVP (RB highly))
      (VP (VBP suggest)
        (NP (CC either)
          (NP (DT the) (JJ hot) (NN chocolate))
          (CC or)
          (NP (DT the) (JJ nutty) (NN professor)))))
    (. .)))

(ROOT
  (S
    (NP
      (NP (JJ Free) (NNS wifi))
