      (S
        (VP (VBG talking)
          (PP (TO to)
            (NP (NN slot) (NNS players)))
          (S
            (VP (TO to)
              (VP (VB pay)
                (NP (NN attention))
                (PP (TO to)
                  (NP (NN anyone) (RB else)))))))))
    (. .)))


