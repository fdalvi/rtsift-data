          (NP (PRP$ my) (NN kiddo))
          (, ,)
          (SBAR
            (WHNP (WDT which))
            (S
              (ADVP (RB totally))
              (VP (VBD made)
                (NP (PRP$ our) (NN day) (NN :-RRB-))))))))
    (. .)))

