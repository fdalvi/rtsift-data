                      (VP (TO to)
                        (VP (VB relax)
                          (CC and)
                          (VB read)
                          (NP (DT a) (NN book)))))))))))))
    (. .)))

(ROOT
  (S
    (NP (PRP You))
    (VP (MD ca) (RB n't)
      (VP (VB do)
        (NP (DT that))
        (PP (IN at)
          (NP (NNP Starbucks)))))
    (: ...)))


