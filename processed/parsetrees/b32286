          (NP (NNS times)))))
    (. .)))

(ROOT
  (S
    (S
      (NP (DT The) (NN service))
      (VP (VBZ is)
        (ADJP (JJ horrible))))
    (CC and)
    (S
      (NP (DT the) (NN food))
      (VP (VBZ is)
        (ADJP (JJ mediocre))))
    (. .)))


