    (NP
      (NP (DT The) (JJ only) (NN reason))
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP give)
            (NP (PRP it))
            (NP
              (NP (CD 4))
              (PP (RB out) (IN of)
                (NP (CD 5))))))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (S
            (NP (PRP they))
            (VP (VBP stop)
              (NP (VBG seating) (NN way))
              (PP (IN before)
                (NP (PRP$ their) (VBN advertised) (NN closing) (NN time)))))
          (CC and)
          (S
            (NP (PRP they))
            (VP (VBP 're)
              (ADVP (RB also))
              (RB not)
              (VP
                (ADVP (RB always))
                (JJ open)
                (PP (IN during)
                  (NP (PRP$ their) (VBN listed) (NNS hours)))))))))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 've)
      (VP (VBN resorted)
        (PP (TO to)
          (S
            (VP
              (VP
                (ADVP (RB always))
                (VBG calling))
              (CC and)
              (VP (VBG placing)
                (S
                  (VP (VB take)
                    (PRT (RP out))
                    (S
                      (VP (TO to)
                        (VP (VB be)
                          (ADJP (JJ safe)))))))))))))
    (. .)))


