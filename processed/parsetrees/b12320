(ROOT
  (S
    (S
      (NP (PRP I))
      (ADVP (RB DO))
      (VP (VBP WANT)
        (S
          (VP (TO TO)
            (VP (VB SAY)
              (NP (DT THE) (NNP STAFF)
                (CC AND)
                (NNP MANAGEMENT) (NNP ARE) (NN IMPECCABLE)))))))
    (: ...)
