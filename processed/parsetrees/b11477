        (VP (VB shop)
          (ADVP (RB here))
          (ADVP
            (ADVP (RB as) (RB often))
            (SBAR (IN as)
              (S
                (NP (PRP I))
                (VP (MD 'd)
                  (VP (VB like)
                    (PP (TO to))))))))))
    (. .)))


