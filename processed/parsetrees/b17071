      (NP (PRP it))
      (VP (VBZ 's)
        (ADJP (JJ impossible)
          (S
            (VP (TO to)
              (VP (VB get)
                (NP (DT a) (JJ good) (NN seat))
                (ADVP (RB alone))))))))
    (. .)))

(ROOT
  (S
    (NP (PRP I))
    (VP (MD would) (RB rather)
      (VP
        (VP (VB pay)
          (NP
            (NP (DT a) (NN bit))
            (JJR more)))
        (CC and)
        (VP (VB go)
          (PP (TO to)
            (NP (NN amc))))))
    (. .)))


