              (VP (TO to)
                (VP (VB go)
                  (CC and)
                  (VB work))))))))
    (. .)))

(ROOT
  (S (CC Plus)
    (NP (EX there))
    (VP (VBZ 's)
      (NP
        (NP (JJ free) (NNS wifi))
        (PP (IN in)
          (NP (PDT all) (PRP$ their) (NNS stores)))
        (PP (VBG including)
          (NP (DT this) (NN one)))))
    (. .)))


