    (NP (RB then))
    (VP (VBP do)
      (VP (VB go)
        (CC and)
        (VB see)
        (SBAR
          (WHNP (WP what))
          (S
            (VP (VBZ 's)
              (PP (IN on)
                (NP (NN offer))))))))
