    (CC and)
    (S
      (NP (PRP I))
      (VP
        (VP (VBP am)
          (ADJP (JJ shocked)))
        (: ...)
        (VP (VBD SHOCKED))
        (: ...)
        (S
          (VP (TO to)
            (VP (RB not) (VB see)
              (NP (JJR more) (JJ 5-star) (NNS reviews)))))))
    (. .)))


