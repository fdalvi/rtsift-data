    (VP (VBP do) (RB not)
      (VP (VB get)
        (NP (NN food) (NN poisoning))
        (PP (IN from)
          (SBAR
            (WHNP (WP what))
            (S
              (NP (PRP I))
              (VP (VBD did)
                (VP (VB ingest))))))))
    (. .)))


