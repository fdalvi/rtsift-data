(ROOT
  (S (CC And)
    (NP (PRP I))
    (ADVP (RB really))
    (VP (VBP do)
      (ADVP (RB quite)
        (PP (IN like)
          (NP (PRP$ my) (JJ new) (NNS shoes)))))
    (. .)))


