          (PP (IN into)
            (NP (DT a) (JJ top) (CD 20) (NN list))))))
    (. .)))

(ROOT
  (S
    (NP (PRP They))
    (VP (VBD used)
      (S
        (VP (TO to)
          (VP (VB be)
            (ADJP
              (ADJP (RB much) (JJR better))
              (CC and)
              (ADJP (JJR faster)))))))
    (. .)))


