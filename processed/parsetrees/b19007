              (VP (TO to)
                (VP (VB fit)
                  (NP (PRP me)))))))))
    (. .)))

(ROOT
  (S
    (NP (NNP Fit))
    (VP
      (VP (VBP like)
        (NP (DT a) (NN glove)))
      (CC and)
      (VP (VBD looked)
        (S
          (ADJP (JJ great)))))
    (. !)))

