            (NP (PRP them))))))
    (. .)))

(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT this) (NN place))
        (VP (VBZ is)
          (ADVP (RB still))
          (ADJP (JJ open)))))
    (NP (PRP I))
    (VP (VBP urge)
      (S
        (NP (PRP you))
        (RB not)
        (VP (TO to)
          (VP (VB go)))))
    (. .)))


