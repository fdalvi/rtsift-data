                      (NP (DT a) (NN while))
                      (PP (IN at)
                        (NP (DT the) (NN drive) (NN thru)))))))))))
      (, ,))
    (ADVP (CC plus))
    (NP (PRP they))
    (VP (VBP do) (RB n't)
      (ADVP (RB even))
      (VP (VB have)
        (NP (DT a) (JJ full) (NNP Burger) (NNP King) (NN menu))))
    (. .)))


