                        (ADJP (JJ worth)
                          (NP (DT the) (NN drive)))
                        (SBAR (IN if)
                          (S
                            (NP (PRP they))
                            (VP (VB end)
                              (PRT (RP up))
                              (S
                                (VP (VBG being)
                                  (VP (VBN closed)))))))))))))))))
    (. .)))


