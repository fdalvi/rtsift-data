                      (PP (IN as)
                        (NP (NN time) (NNS permits))))))))))))
    (. .)))

(ROOT
  (S
    (NP (PRP You))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB try)
            (NP
              (NP
                (NP (NNP Nicole) (POS 's))
                (NN carrot) (NN cake))
              (: -)
              (NP (NNP OMG)))))))
    (. .)))

(ROOT
  (S (CC And)
    (S
      (NP (PRP$ her) (NN specialty) (NN ice) (NN cream) (NNS flavors))
      (: -)
      (S
        (VP (VBG putting)
          (NP (NNS ingredients))
          (ADVP (RB together))))
      (NP (PRP I))
      (VP (MD would)
        (ADVP (RB never))
        (VP (VB think)
          (PP (IN of)))))
    (CC and)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (NP (NN soooo) (NN yummy))))
    (. .)))


